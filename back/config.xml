    <field type="text" name="staticDirAddr" label="Static Directions Address" description="If enabled static directions displays a link that will show directions to a fixed point from the location." />
    <!--<field type="text" name="staticDirAddr" label="STATIC_DIR_ADDR_LABEL" description="STATIC_DIR_ADDR_DESC" />-->
    <field type="text" name="staticDirLabel" label="Static Directions Link Label" description="A short description of the end point. Will be appended to Directions to to create a link. (e.g. Directions to LAX)" />
