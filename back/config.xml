      <option value="1">On</option>
    </field>
  </fieldset>
  <fieldset name='wmp_static' label='Static Directions' description=''>
    <!--<param name="@spacer" type="spacer" default="" label="" description="" menu="hide"/>
    <param name="@webmapplus" type="webmapplushead" default="STATIC_DIRECTIONS_LABEL"/>-->
    <field name="staticDir" type="radio" default="0" label="Static Directions" description="If enabled static directions displays a link that will show directions to a fixed point from the location.">
