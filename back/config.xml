  <fieldset name='wmp_gm' label='Google Maps Settings' description=''>
    <!--<field name="@webmapplus" type="webmapplushead" default="GOOGLE_MAPS_SETTINGS_LABEL" menu="hide"/>-->
    <field type="gmapskey" name="gmaps_api_key" size="90" label="Google Maps API Key" description="Google Mapis API Key for your domain" menu="hide"/>
    <field name="map_controls" type="list" default="auto" label="Pan/Zoom Control Type" description="Pan/Zoom Control Type and Visibility">
      <option value="auto">Auto</option>
      <option value="small">Small</option>
      <option value="large">Large</option>
      <option value="none">None</option>
    </field>
    <field name="map_basic_map" type="radio" default="1" label="Basic Map Button" description="Visibility for the standard map button (default map view)">
      <!--BASIC_MAP_BUTTON_DESC-->
      <option value="0">Hide</option>
      <option value="1">Show</option>
    </field>
    <field name="map_satellite_map" type="radio" default="1" label="Satellite Map" description="Visibility for the satellite map (if hidden, will not be able to view map in this style)">
      <!--<field name="map_satellite_map" type="radio" default="1" label="Satellite Map" description="SATELLITE_MAP_DESC">-->
      <option value="0">Hide</option>
      <option value="1">Show</option>
    </field>
    <field name="map_hybrid_map" type="radio" default="1" label="Hybrid Map Button" description="Visibility for the hybrid map (if hidden, will not be able to view map in this style)">
      <!--<field name="map_hybrid_map" type="radio" default="1" label="Hybrid Map Button" description="HYBRID_MAP_DESC">-->
      <option value="0">Hide</option>
      <option value="1">Show</option>
    </field>
    <field name="map_physical_map" type="radio" default="0" label="Physical Map Button" description="Visibility for the physical map (if hidden, will not be able to view map in this style)">
      <!--<field name="map_physical_map" type="radio" default="0" label="Physical Map Button" description="PHYSICAL_MAP_DESC">-->
      <option value="0">Hide</option>
      <option value="1">Show</option>
    </field>
    <field name="map_scroll_wheel" type="radio" default="1" label="Google Map Scroll Wheel" description="Scroll Wheel on Mouse will control Map Zoom">
      <option value="0">Disable</option>
      <option value="1">Enable</option>
    </field>
    <field name="map_scalecontrol" type="radio" default="1" label="Measurement Scale Control" description="Measurement Scale Control">
      <option value="0">Hide</option>
      <option value="1">Show</option>
    </field>
  </fieldset>
  <fieldset name='wmp_gml' label='Google Maps Layer' description=''>
    <!--<field name="@spacer" type="spacer" default="" label="" description="" menu="hide"/>
    <param name="@webmapplus" type="webmapplushead" default="GOOGLE MAPS LAYER"/>-->
    <field name="wikipedia_layer" type="list" default="auto" label="Wikipedia Layer" description="Include Geotagged Wikipedia Landmarks in your map">
      <option value="none">None</option>
      <option value="en">English</option>
      <option value="es">Spanish</option>
      <option value="fr">French</option>
    </field>
    <field name="wikipedia_zoom_layer" type="list" default="15" label="Wikipedia Zoom Level" description="The zoom level at which the Wikipedia layer will be disabled, 18 is the max zoom level, setting the max zoom level to 1 is equivalent to turning off the layer">
      <!--<field name="wikipedia_zoom_layer" type="list" default="15" label="Wikipedia Zoom Level" description="WIKIPEDIA_ZOOM_LEVEL_DESC">-->
      <option value="1">Zoom Level 1</option>
      <option value="2">Zoom Level 2</option>
      <option value="3">Zoom Level 3</option>
      <option value="4">Zoom Level 4</option>
      <option value="5">Zoom Level 5</option>
      <option value="6">Zoom Level 6</option>
      <option value="7">Zoom Level 7</option>
      <option value="8">Zoom Level 8</option>
      <option value="9">Zoom Level 9</option>
      <option value="10">Zoom Level 10</option>
      <option value="11">Zoom Level 11</option>
      <option value="12">Zoom Level 12</option>
      <option value="13">Zoom Level 13</option>
      <option value="14">Zoom Level 14</option>
      <option value="15">Zoom Level 15</option>
      <option value="16">Zoom Level 16</option>
      <option value="17">Zoom Level 17</option>
      <option value="18">Zoom Level 18</option>
    </field>
    <field name="panoramio_layer" type="radio" default="1" label="Panoramio Photo Layer" description="Display all photos from panoramio.com in your map">
      <!--<field name="panoramio_layer" type="radio" default="1" label="Panoramio Photo Layer" description="PANORAMIO_LAYER_DESC">-->
      <option value="0">Hide</option>
      <option value="1">Show</option>
    </field>
    
  </fieldset>
  <fieldset name='wmp_loc_map' label='Location Map Settings' description=''>
    <!--<param name="@spacer" type="spacer" default="" label="" description="" menu="hide"/>
    <param name="@webmapplus" type="webmapplushead" default="WEBMAPPLUS_LOCATIONS_MAP_SETTINGS_LABEL" menu="hide"/>-->
    <field name="map_category_filter" type="radio" default="0" label="Category Filter" description="Show category filter, allows user to select only a certain category of locations">
